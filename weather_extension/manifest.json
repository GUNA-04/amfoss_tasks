
{
    "manifest_version": 3,
    "name": "Weather Extension",
    "version": "1.0",
    "description": "Displays current weather information of the selected city.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon2.png"
    },
    "permissions": [
      "geolocation"
    ]
  }
  